/**app.wxss**/

page {
  display: flex;
  flex-flow: column nowrap;
  min-height: 100%;
}

view {
  display: flex;
  width: 100%;
}

view, image, navigator, textarea {
  box-sizing: border-box;
}

swiper {
  width: 100%;
  position: relative;
}

.box {
  flex: 1;
  flex-direction: column;
  overflow-y: auto;
}

@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1582786538597'); /* IE9 */
  src: url('iconfont.eot?t=1582786538597#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
         url('iconfont.woff?t=1582786538597') format('woff'),
         url('iconfont.ttf?t=1582786538597') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+ */ url('iconfont.svg?t=1582786538597#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

